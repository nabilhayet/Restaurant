<%= form_for [@admin, @cafe] do |f| %>
    <%= f.hidden_field :admin_id %>
    <br>
    <br>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :street %>
    <%= f.text_field :street %>
    <br>
    <br>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <br>
    <br>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <br>
    <br>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
    <br>
    <br>
    <%= f.submit %>
  <% end %>