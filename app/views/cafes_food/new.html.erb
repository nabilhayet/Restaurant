<%= form_for @cf, url: cafe_cafes_food_index_path(@cafe) do |f| %>
    Cafe : <%= f.collection_select :cafe_id, Cafe.all, :id, :name %>
    <br>
    <br>
    Food : <%= f.collection_select :food_id, Food.all, :id, :name %>
    <br>
    <br>
    <%= f.label :price %>
    <%= f.number_field :price %>
    <br>
    <br> 
    <%= f.button "Make a Cafe Food", :class => 'btn' %>
<% end %> 